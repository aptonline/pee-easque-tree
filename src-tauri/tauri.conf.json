{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "pee-esque-tree",
  "version": "0.1.0",
  "identifier": "me.aptly.pee-esque-tree",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Pee-esque-tree - Game Update Downloader",
        "width": 800,
        "height": 640,
        "resizable": false,
        "fullscreen": false,
        "hiddenTitle": false,
        "incognito": false
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
